{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug app sales order",
      "request": "launch",
      "type": "node",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npx",
      "env": {
        "NODE_ENV": "development"
      },
      "runtimeArgs": [
        "cds-tsx",
        "watch",
      ],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "sourceMaps": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
    }
  ]
}